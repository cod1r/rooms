{
	"useTabs": true,
	"indentWidth": 1,
	"incremental": true,
	"typescript": {
		"quoteStyle": "alwaysSingle"
	},
	"json": {
	},
	"includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json}"],
	"excludes": [
		"**/node_modules",
		"**/*-lock.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.61.0.wasm",
		"https://plugins.dprint.dev/json-0.14.0.wasm"
	]
}
